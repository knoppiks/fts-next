{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:best-practices"
  ],
  "prConcurrentLimit": 3,
  "packageRules": [
    {
      "matchPackagePatterns": [
        "com.fasterxml.jackson"
      ],
      "groupName": "jackson"
    },
    {
      "matchPackagePatterns": [
        "ca.uhn.hapi.fhir"
      ],
      "groupName": "hapi"
    }
  ]
}
